{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/angular-web-app"
        }
      }
    ],
    "routes": [
        {
            "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
        }
    ]
  }
  